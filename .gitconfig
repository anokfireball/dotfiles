[user]
	email = github@kthxbye.cyou
	name = Fabian Koller
[init]
	defaultBranch = main
[pull]
	rebase = true
[rebase]
	autoStash = true
	autoSquash = true
	updateRefs = true
[merge]
	conflictstyle = diff3
[diff]
	colorMoved = default

[includeIf "gitdir:~/"]
	path = ~/.gitalias
	path = ~/.gitconfig_delta_active

[includeIf "gitdir:~/.work/"]
	path = ~/.work/.gitconfig
