[alias]
    a = add
    aa = add --all
    ap = add --patch

    b = branch

    c = commit
    ca = commit --amend
    ci = commit --interactive

    cl = clean -d --force --dry-run
    cln = clean -d --force

    co = checkout
    cob = checkout --branch

    cp = cherry-pick
    cpa = cherry-pick --abort
    cpc = cherry-pick --continue

    d = diff
    ds = diff --staged

    f = fetch
    fa = fetch --all

    g = grep

    l = log --graph --pretty=format:'%C(yellow)%h %Cred%ad %Cblue%an%C(auto)%d %Creset%s' --date=short
    lg = log --graph
    lm = log --medium
    lp = log --patch

    m = merge
    ma = merge --abort
    mc = merge --continue

    p = push
    pd = push --delete
    ps = push --set-upstream
    pt = push --tags

    pu = pull

    r = rebase
    ra = rebase --abort
    rc = rebase --continue
    ri = rebase --interactive

    rss = reset --soft
    rsh = reset --hard

    s = status --short --branch
    sl = status --long
    sv = status --verbose

    st = stash
    std = stash drop
    stl = stash list
    stp = stash pop

    t = tag
    td = tag --delete
    tl = tag --list
